<div class="finder-car-content-inner">
  <h3 class="heading-e">All car models:</h3>
  <div class="variants variants-4">
    {{#if this.value.values}}
      {{#slices this.value.values size=4}}
        <div class="variants-group">
          {{#each this}}
              <input type="radio" name="{{#if ../../queryInfo.model}}sub{{/if}}model-radio" id="{{#if ../../queryInfo.model}}sub{{/if}}model-{{this}}">
              <label class="variant variant-centered" for="{{#if ../../queryInfo.model}}sub{{/if}}model-{{this}}">
                <span>{{this}}</span>
              </label>
          {{/each}}
        </div>
      {{/slices}}
    {{else}}
      <h6 class="heading-f">No data found.</h6>
    {{/if}}
  </div>
</div>
